{
  "name": "vscode-text-combiner",
  "displayName": "VSC Combine Open Files",
  "description": "Simply combine contents of all open editors into one file with one click.",
  "version": "0.0.2",
  "publisher": "Ideas2Byte",
  "engines": {
    "vscode": "^1.70.0"
  },
  "main": "./out/extension.js",
  "activationEvents": [
    "onCommand:extension.combineOpenFiles"
  ],
  "contributes": {
    "commands": [
      {
        "command": "extension.combineOpenFiles",
        "title": "Combine Open Files"
      }
    ]
  },
  "icon": "icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/thewhisperinyourears/VScombiner"
  },
  "license": "MIT",
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.70.0",
    "@vscode/test-electron": "^2.5.2",
    "typescript": "^4.8.4"
  }
}
